# Backend Environment Variables - Copy to .env and update as needed

# Hugging Face Cache (optional - speeds up model loading)
HF_HOME=D:\AI_CACHE\huggingface

# Torch Cache (optional - for model weights)
TORCH_HOME=D:\AI_CACHE\torch

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=False
SECRET_KEY=your-secret-key-here

# API Configuration
API_HOST=0.0.0.0
API_PORT=5000
